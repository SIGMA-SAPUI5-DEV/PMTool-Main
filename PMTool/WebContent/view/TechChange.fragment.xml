<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form">
	<Dialog
		id="idTechDialogChange"
		contentWidth= "800px"
		contentHeight= "600px"
		resizable= "true"
		title="Maintain Technical Tracker"
		draggable="true">
		<content>
			<f:SimpleForm>
				<Label text = "Project Id" />
				<!--<Input id = "inProjectId"/>-->
				<ComboBox id="inProjectId" width="7cm" showSecondaryValues="true" items="{ path: '/ProjMasterSet'}" selectionChange="handle_projId_combo">
					<core:Item key="{ZprojId}" text="{ZprDesc}"/>
				</ComboBox>
				<Label text = "Team" /> <Input id = "inTeam"/>
				<Label text = "Gap No" /> <Input id = "inGapNo"/>
				<Label text = "Spec Name" /> <Input id = "inSpecName"/>
				<Label text = "Spec Version" /> <Input id = "inSpecVer"/>
				<Label text = "Spec Descriprion" /> <Input id = "inSpecDesc"/>
				<Label text = "Sequence Number" /> <Input id = "inSeq"/>
				<Label text = "Ticket" /> <Input id = "inTicket"/>
				<Label text = "Category" /> <Input id = "inCategory"/>
				<Label text = "Development Type" /> <Input id = "inDevType"/>
				<Label text = "Dev Scope" /> <Input id = "inDevScope"/>
				<Label text = "Assigned by" /> <Input id = "inAssignedBy"/>
				<Label text = "Assigned date" /> 
					<DatePicker id = "inAssignedDate"
								placeholder="Enter Date ..."
    							class="sapUiSmallMarginBottom"
    							displayFormat="dd-MM-yyyy"
    							valueFormat="dd-MM-yyyy"/>
				<Label text = "Planned Start" /> 
					<DatePicker  id = "inPlanStart"
    							placeholder="Enter Date ..."
    							class="sapUiSmallMarginBottom"
    							displayFormat="dd-MM-yyyy"
    							valueFormat="dd-MM-yyyy"/>    							
    							
				<Label text = "Planned End" /> 
					<DatePicker id = "inPlanEnd"
    							placeholder="Enter Date ..."
    							class="sapUiSmallMarginBottom"
    							displayFormat="dd-MM-yyyy"
    							valueFormat="dd-MM-yyyy" />    							
				<Label text = "Developer" /> <Input id = "inDeveloper"/>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button
				text="Save"
				type="Accept"
				press="fTechSave_Change"/>
		</beginButton>
		<endButton>
			<Button
				text="Cancel"
				press="onCloseDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>
